{
  "name": "uni-ui-snippets-vscode",
  "displayName": "uni-ui-snippets",
  "version": "0.6.5",
  "description": "uni-ui 基本能力代码片段。",
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "uniapp",
    "uni-app",
    "uni-ui",
    "uniui",
    "snippet",
    "snippets"
  ],
  "homepage": "https://github.com/uni-helper/uni-ui-snippets-vscode#readme",
  "bugs": {
    "url": "https://github.com/uni-helper/uni-ui-snippets-vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uni-helper/uni-ui-snippets-vscode"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "publisher": "uni-helper",
  "type": "module",
  "scripts": {
    "check:deps": "ncu",
    "check:types": "tsc --noEmit",
    "lint": "run-p lint:eslint lint:markdownlint check:types",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue --ignore-path=.gitignore --cache",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "release": "release-it",
    "prepare": "is-ci || husky install",
    "publish": "run-p publish:vscode publish:openvsx",
    "publish:vscode": "vsce package && vsce publish",
    "publish:openvsx": "ovsx publish"
  },
  "contributes": {
    "snippets": [
      {
        "language": "vue-html",
        "path": "./snippets/vue-html.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue-html.json"
      },
      {
        "language": "html",
        "path": "./snippets/vue-html.json"
      },
      {
        "language": "css",
        "path": "./snippets/css.json"
      },
      {
        "language": "less",
        "path": "./snippets/css.json"
      },
      {
        "language": "scss",
        "path": "./snippets/css.json"
      },
      {
        "language": "sass",
        "path": "./snippets/css.json"
      },
      {
        "language": "stylus",
        "path": "./snippets/css.json"
      },
      {
        "language": "vue",
        "path": "./snippets/css.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "vue",
        "path": "./snippets/javascript.json"
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@commitlint/prompt": "^17.3.0",
    "@modyqyw/fabric": "^5.14.0",
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "^18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "commitizen": "^4.2.6",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "markdownlint-cli": "^0.32.2",
    "npm-check-updates": "^16.6.0",
    "npm-run-all": "^4.1.5",
    "ovsx": "^0.7.1",
    "prettier": "^2.8.1",
    "release-it": "^15.5.1",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4",
    "vsce": "^2.14.0"
  },
  "packageManager": "npm@9.2.0",
  "engines": {
    "node": ">=14.18",
    "vscode": "^1.40.0"
  },
  "icon": "uni-helper.png",
  "sponsor": {
    "url": "https://www.paypal.me/wurui7"
  }
}
